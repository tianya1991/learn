<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="OracleTable" targetRuntime="mybatis3" defaultModelType="flat">
        <!-- 生成的Java文件的編碼 -->
        <property name="javaFileEncoding" value="UTF-8"/>
        <!-- 格式化java代碼 -->
        <property name="javaFormatter" value="org.mybatis.generator.api.dom.DefaultJavaFormatter"/>

        <!-- 数据库关键字冲突,自动处理 -->
        <property name="autoDelimitKeywords" value="true"/>
        <!-- 用反引号`包裹,默认是双引号"--><!--
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>-->

        <plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
            <property name="searchString" value="Example$"/>
            <property name="replaceString" value="Criteria"/>
        </plugin>
        <!--<plugin type="org.mybatis.generator.plugins.OverIsMergeablePlugin">-->
        <!--<property name="isMergeable" value="false"/>-->
        <!--</plugin>-->
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
        <!--<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin"/>-->
        <plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin"/>
        <!--<plugin type="org.mybatis.generator.plugins.ToStringPlugin"/>-->
        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin"/>
        <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin"/>

        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
            <property name="suppressDate" value="true"/>
            <property name="addRemarkComments" value="true"/>
            <property name="useInformationSchema" value="true"></property>
        </commentGenerator>

        <jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver"
                        connectionURL="jdbc:oracle:thin:@192.168.1.101:1521/ORCL"
                        userId="c##root"
                        password="root">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- generate Model -->
        <javaModelGenerator targetPackage="com.example.studentdemo.datasource.entity"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="false"/>
            <property name="trimStrings" value="true"/>
            <!--<property name="immutable" value="false"/>-->
<!--            <property name="rootClass" value="com.example.qin.platform.core.entity.BaseEntity"/>-->
<!--            <property name="rootInterface" value="com.example.qin.platform.core.basics.IdAndName"/>-->
        </javaModelGenerator>


        <sqlMapGenerator targetPackage="com.example.studentdemo.datasource.dao" targetProject="src/main/resources">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>


        <javaClientGenerator type="XMLMAPPER" targetPackage="com.example.studentdemo.datasource.dao"
                             targetProject="src/main/java">
            <property name="enableSubPackages" value="false"/>
            <property name="addMapperAnnotation" value="true"/>
        </javaClientGenerator>


        <!--自动化事件信息表-->

        <table tableName="STUDENT" domainObjectName="Student">
            <property name="ignoreQualifiersAtRuntime" value="true"/>
        </table>
        <!--<table tableName="STUDENT" domainObjectName="FlowModifyRecord">-->
            <!--<property name="ignoreQualifiersAtRuntime" value="true"/>-->
        <!--</table>-->
        <!--<table tableName="STUDENT" domainObjectName="FlowFormData">-->
            <!--<property name="ignoreQualifiersAtRuntime" value="true"/>-->
        <!--</table>-->

    </context>
</generatorConfiguration>
